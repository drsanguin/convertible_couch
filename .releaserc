{
  "branches": [
    "main",
    "release/*",
    { "name": "ci/semantic-release", "prerelease": false }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": [
          { "path": "convertible_couch.zip" }
        ]
      }
    ]
  ]
}
